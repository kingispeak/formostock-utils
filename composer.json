{
  "name": "formostock/utils",
  "description": "Formostock Laravel Utils Package",
  "keywords": [
    "Formostock",
    "laravel",
    "utils"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/kingispeak/formostock-utils",
  "authors": [
    {
      "name": "Sin-Fong Lyu",
      "email": "kingispeak@gmail.com",
      "homepage": "https://github.com/kingispeak",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "require": {
    "php": ">=7.1.3"
  },
  "require-dev": {
    "laravel/legacy-factories": "^1.1",
    "orchestra/testbench": "^5.0|^6.0",
    "orchestra/database": "^5.0|^6.0"
  },
  "autoload": {
    "psr-4": {
      "Formostock\\Utils\\": "src/"
    },
    "classmap": []
  },
  "autoload-dev": {
    "psr-4": {
      "Formostock\\Utils\\Test\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Formostock\\Utils\\ServiceProvider"
      ]
    }
  }
}
